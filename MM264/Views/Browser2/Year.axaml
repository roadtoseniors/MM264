<wpflikeavalonianavigation:Page xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:wpflikeavalonianavigation="using:WpfLikeAvaloniaNavigation"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MM264.Year">
    <Grid RowDefinitions="Auto, *" Margin="16">
		<TextBlock Text="Год" FontSize="18" FontWeight="Bold" Margin="0,0,0,12"/>

        <Grid Grid.Row="1" ColumnDefinitions="*,*,*,*" ColumnSpacing="8" RowDefinitions="*,*,*" RowSpacing="8">
			<Border Grid.Column="0" Grid.Row="0" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Январь" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="1" Grid.Row="0" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Февраль" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="2" Grid.Row="0" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Март" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="3" Grid.Row="0" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Апрель" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="0" Grid.Row="1" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Май" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="1" Grid.Row="1" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Июнь" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="2" Grid.Row="1" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Июль" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="3" Grid.Row="1" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Август" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="0" Grid.Row="2" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Сентябрь" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="1" Grid.Row="2" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Октябрь" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="2" Grid.Row="2" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Ноябрь" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
            <Border Grid.Column="3" Grid.Row="2" BorderThickness="1" BorderBrush="Gray" CornerRadius="6" Padding="8">
				<TextBlock Text="Декабрь" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            </Border>
        </Grid>
    </Grid>
</wpflikeavalonianavigation:Page>
